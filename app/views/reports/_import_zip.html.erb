<div class="py-5">
  <h1>Importar arquivo ZIP</h1>

  <%= form_with url: import_zip_reports_path, method: :post, local: true, html: { multipart: true } do |form| %>
      <div class="form-group">
        <strong><%= form.label :zip_file, 'Escolha um arquivo ZIP para upload:' %></strong>
        <%= form.file_field :zip_file %>
      </div>
      <div class="actions">
        <%= form.submit 'Importar ZIP', class: 'btn btn-primary' %>
      </div>
  <% end %>
</div>
